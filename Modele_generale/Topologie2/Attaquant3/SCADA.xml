int sk:=6; //private key
int pk_client:=2; // public key of client
bool write; // if write==false then is a command of reading else is a command of writing

int action[2]:={3,4}; // The OPC-UA server controls system variables 3(nozzle) and 4(Level) 
int nbaction:=2; // the numbre of variables
//message
struct{
    int  variable;
    int  function;
    int  value;
    int  s_variable;
    int  s_function;
    int  s_value;
      
} data;

// Check if the server has access to the variable or no
bool search(int action[2],int var){
int i,j;
bool res:=false;
for(i:=0; i&lt;nbaction;i++){
         
    if(action[i]==var) res:=true;
    else res:=false;

}

return res;
}

// Check whether a message is for reading or writing
void CheckMsg(){

if(data.function==1){
    
    write:=1;

}
else{

    write:=0;

}
}

// Answer on a read command

void Update(){

    msg.value := System[data.variable];

}

// Execute a command
void Execute(){

    if(search(action,data.variable)==true)
    System[data.variable]:= data.value;

