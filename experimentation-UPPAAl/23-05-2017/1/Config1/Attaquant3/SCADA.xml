		<declaration>void SignMsg(){
 
 tram.s_variable := Sign(tram.variable,private_key);
 tram.s_function := Sign(tram.function,private_key);
 tram.s_value    := Sign(tram.value,private_key);
 //
 public_key:=0;
 private_key:=0;

}

void SignAndCryptMsg(){

 tram.variable   := Encrypt(public_key,tram.variable);
 tram.function   := Encrypt(public_key,tram.function);
 tram.value      := Encrypt(public_key,tram.value);
 tram.s_variable := Encrypt(public_key,Sign(tram.variable,private_key));
 tram.s_function := Encrypt(public_key,Sign(tram.function,private_key));
 tram.s_value    := Encrypt(public_key,Sign(tram.value,private_key));
 //
 public_key:=0;
 private_key:=0;
}

void DecryptMsg(){

 tram.variable   := Decrypt(private_key,tram.variable);
 tram.function   := Decrypt(private_key,tram.function);
 tram.value      := Decrypt(private_key,tram.value);
 tram.s_variable := Decrypt(private_key,tram.s_variable);
 tram.s_function := Decrypt(private_key,tram.s_function);
 tram.s_value    := Decrypt(private_key,tram.s_value);
 //
 public_key:=0;
 private_key:=0;
}

void CheckSignMsg(){
 
if(CheckSign(tram.s_variable,tram.variable,public_key) and CheckSign(tram.s_function,tram.function,public_key) 
   and CheckSign(tram.s_value,tram.value,public_key)){check:=true;}
else {check:=false;}
 //
 public_key:=0;
 private_key:=0;
}

