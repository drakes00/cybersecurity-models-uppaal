//Channels

chan msg,c2s,s2c,read,sign,signAndCrypt,decrypt,checkSign;
chan open,close,start,stop;
broadcast chan on,off,chan1,chan2;

// message

struct{
    int  variable;
    int  function;
    int  value;
    int  s_variable;
    int  s_function;
    int  s_value;
} tram;

//System variable

bool run:=0;    // 1
bool motor:=0;  // 2
bool bottle:=0; // 3
bool nozzle:=0; // 4
bool level:=0;  // 5

//**************************************************************************************************************
//                                                                                                             *   
//                                    Functions for encrypt and decrypt messages                               *    
//                                                                                                             *   
//**************************************************************************************************************

// Hash function
int Hash(int msg){

 return msg*5;

}
//Encryption function
int Encrypt(int p,int m){

   return (m+(2*p));

}
//Decryption function
int Decrypt(int s,int m){

 return (m-s);

}
// Sign function
int Sign(int m,int s){

    int d;
    d:=Hash(m);
    return (d+s);

}
// CHeck sign function
bool CheckSign(int sign,int m, int p){

   int res_hash,hash;
   hash := Decrypt(2*p,sign);
   res_hash := Hash(m);
   
   if(hash==res_hash) return true;
   else return false;

}
